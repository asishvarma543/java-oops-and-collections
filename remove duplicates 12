import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String args[])
    {
        Scanner in = new Scanner(System.in);
        String s=in.nextLine();
        Set<Character> unique=new LinkedHashSet<>();
        for(int i=0;i<s.length();i++)
        {
            char c = s.charAt(i);
            unique.add(c);
        }
        for(Character l:unique)
        System.out.print(l);
    }
}

                
        
